#!/command/with-contenv bash
#shellcheck shell=bash

set -eo pipefail

s6wrap --quiet --timestamps --prepend="$(basename "$0")" --args echo "Stats are temporarily disabled."
sleep infinity

sleep 5

# shellcheck disable=SC2016
s6wrap --quiet --timestamps --prepend="$(basename "$0")" --args python3 -u /scripts/stats.py 2>/dev/null